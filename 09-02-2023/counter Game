public static String counterGame(long n) {
    // Write your code here
        int count=0;
        while(n>0 && n%2==0)
        {
            count++;
            n=n/2;
        }

        while(n>0)
        {
            if(n%2==1)
            {
                count++;
            }
            n=n/2;
        }
        if(count%2==1)
        {
            return "Richard";
        }
        else
        {
            return "Louise";
        }
  }
}
